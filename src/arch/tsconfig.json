{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "module": "amd",
    "newLine": "LF",
    "target": "es2017",
    // "sourceMap": false,
    "inlineSourceMap": true,
    "removeComments": true,
    "lib": [
      "dom",
      "es2017"
    ],
    "rootDir": "../../",
    "outFile": ".gen-compiled.js"
  },
  "include": [
    "../global.d.ts",
    "gen.ts"
  ]
}
